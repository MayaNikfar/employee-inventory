<%- include('../partials/top') %>

<h1>Employees List</h1>
<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Full Name</th>
      <th> Employee ID</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% employees.forEach(function(e) { %>
      <tr>
        <td><%= e.name %></td>
        <td><%= e.employeeId %></td>
        <td><a href="/employees/<%= e._id %>">Detail</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/bottom') %>