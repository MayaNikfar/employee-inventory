<%- include('../partials/top') %>

<h1>Employees List</h1>
<div class="relative">
<table class="table table-striped table-hover position-absolute top-50 start-50 translate-middle">
  <thead>
    <tr>
      <th>Full Name</th>
      <th> Employee ID</th>
      
    </tr>
  </thead>
  <tbody>
    <% employees.forEach(function(e) { %>
      <tr>
        <td><%= e.name %></td>
        <td><%= e.employeeId %></td>
        <td class="a"><a href="/employees/<%= e._id %>">Detail</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
</div>  
<%- include('../partials/bottom') %>